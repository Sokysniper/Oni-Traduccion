msgctxt "STRINGS.CODEX.ANALYSIS.SUBTITLE"
msgid "Study of world features"
msgstr "Estudio de las características del mundo"

#. STRINGS.CODEX.ANALYSIS.TITLE
msgctxt "STRINGS.CODEX.ANALYSIS.TITLE"
msgid "<link=\"ANALYSIS\">Geographical Feature Analysis</link>"
msgstr "<link=\"ANALYSIS\">Análisis de Características Geográficas</link>"

#. STRINGS.CODEX.ATMOSPHERE.BODY.CONTAINER1
msgctxt "STRINGS.CODEX.ATMOSPHERE.BODY.CONTAINER1"
msgid "Description of breath, pressure, and gas"
msgstr "Descripción de la respiración, presión y gas"

#. STRINGS.CODEX.ATMOSPHERE.SUBTITLE
msgctxt "STRINGS.CODEX.ATMOSPHERE.SUBTITLE"
msgid "Air and other gasses"
msgstr "Aire y otros Gases"

#. STRINGS.CODEX.ATMOSPHERE.TITLE
msgctxt "STRINGS.CODEX.ATMOSPHERE.TITLE"
msgid "Atmosphere"
msgstr "Atmósfera"

