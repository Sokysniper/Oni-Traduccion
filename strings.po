#. STRINGS.DUPLICANTS.ROLES.MATERIALS_MANAGER.NAME
msgctxt "STRINGS.DUPLICANTS.ROLES.MATERIALS_MANAGER.NAME"
msgid "<link=\"MATERIALSMANAGER\">Courier</link>"
msgstr "<link=\"MATERIALSMANAGER\">Mensajero</link>"

#. STRINGS.DUPLICANTS.ROLES.MECHATRONIC_ENGINEER.DESCRIPTION
msgctxt "STRINGS.DUPLICANTS.ROLES.MECHATRONIC_ENGINEER.DESCRIPTION"
msgid ""
"This job provides additional training in the construction and\n"
"maintenance of conveyor rail systems."
msgstr ""
"Este trabajo proporciona capacitación adicional en la construcción y mantenimiento de sistemas de transportadores ferroviarios."

#. STRINGS.DUPLICANTS.ROLES.MECHATRONIC_ENGINEER.NAME
msgctxt "STRINGS.DUPLICANTS.ROLES.MECHATRONIC_ENGINEER.NAME"
msgid "<link=\"MECHATRONICENGINEER\">Mechatronics Engineer</link>"
msgstr "<link=\"MECHATRONICENGINEER\">Ingeniero Mecatrónico</link>"

#. STRINGS.DUPLICANTS.ROLES.MEDIC.DESCRIPTION
msgctxt "STRINGS.DUPLICANTS.ROLES.MEDIC.DESCRIPTION"
msgid ""
"Becoming a Medic will allow a Duplicant to treat the sick and\n"
"injured with more efficiency."
msgstr "Convertirse en un Medico le permitirá a un Duplicante tratar a los enfermos y lesionados con más eficacia."

#. STRINGS.DUPLICANTS.ROLES.MEDIC.NAME
msgctxt "STRINGS.DUPLICANTS.ROLES.MEDIC.NAME"
msgid "<link=\"MEDIC\">Medic</link>"
msgstr "<link=\"MEDIC\">Medico</link>"

#. STRINGS.DUPLICANTS.ROLES.MINER.DESCRIPTION
msgctxt "STRINGS.DUPLICANTS.ROLES.MINER.DESCRIPTION"
msgid ""
"This job provides training in the excavation of the element\n"
